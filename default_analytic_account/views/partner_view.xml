<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_partner_form_inherit_default_analytic_account" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.default.analytic.account</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="account.view_partner_property_form" />
        <field name="arch" type="xml">
            <xpath expr="//group[@name='accounting_entries']" position="inside">
                <field name="default_analytic_account_id" />
            </xpath>
        </field>
    </record>

</odoo>
