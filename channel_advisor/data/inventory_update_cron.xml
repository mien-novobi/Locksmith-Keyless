<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="ca_product_import" model="ir.cron">
        <field name="name">Channel Advisor: Product Import</field>
        <field name="model_id" ref="model_ca_connector" />
        <field name="state">code</field>
        <field name="code">model._cron_import_products()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">4</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
    </record>


    <record id="ca_quatity_update" model="ir.cron">
        <field name="name">Channel Advisor: Quantity Update</field>
        <field name="model_id" ref="model_ca_connector" />
        <field name="state">code</field>
        <field name="code">model._cron_update_quantity(limit=1000)</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">30</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field name="active" eval="False" />
    </record>


    <record id="ca_update_queue" model="ir.cron">
        <field name="name">Channel Advisor: Quantity Update Queue</field>
        <field name="model_id" ref="model_ca_update_queue" />
        <field name="state">code</field>
        <field name="code">model._cron_process_update_queue(batch_size=80)</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">4</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
    </record>

</odoo>
